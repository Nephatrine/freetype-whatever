/*
 *  This file registers the FreeType modules compiled into the library.
 *
 *  If you use GNU make, this file IS NOT USED!  Instead, it is created in
 *  the objects directory (normally `<topdir>/objs/') based on information
 *  from `<topdir>/modules.cfg'.
 *
 *  Please read `docs/INSTALL.ANY' and `docs/CUSTOMIZE' how to compile
 *  FreeType without GNU make.
 *
 */

#cmakedefine FT2_DRIVER_BDF
#cmakedefine FT2_DRIVER_CID
#cmakedefine FT2_DRIVER_OPENTYPE
#cmakedefine FT2_DRIVER_PCF
#cmakedefine FT2_DRIVER_SFNT
#cmakedefine FT2_DRIVER_TRUEDOC
#cmakedefine FT2_DRIVER_TRUETYPE
#cmakedefine FT2_DRIVER_TYPE1
#cmakedefine FT2_DRIVER_WINFNT
#cmakedefine FT2_SUPPORT_SMOOTH

FT_USE_MODULE( FT_Module_Class, autofit_module_class )
#ifdef FT2_DRIVER_TRUETYPE
FT_USE_MODULE( FT_Driver_ClassRec, tt_driver_class )
#endif
#ifdef FT2_DRIVER_TYPE1
FT_USE_MODULE( FT_Driver_ClassRec, t1_driver_class )
#endif
#ifdef FT2_DRIVER_OPENTYPE
FT_USE_MODULE( FT_Driver_ClassRec, cff_driver_class )
#endif
#ifdef FT2_DRIVER_CID
FT_USE_MODULE( FT_Driver_ClassRec, t1cid_driver_class )
#endif
#ifdef FT2_DRIVER_TRUEDOC
FT_USE_MODULE( FT_Driver_ClassRec, pfr_driver_class )
#endif
#if defined(FT2_DRIVER_TRUETYPE) && defined(FT2_DRIVER_TYPE1)
FT_USE_MODULE( FT_Driver_ClassRec, t42_driver_class )
#endif
#ifdef FT2_DRIVER_WINFNT
FT_USE_MODULE( FT_Driver_ClassRec, winfnt_driver_class )
#endif
#ifdef FT2_DRIVER_PCF
FT_USE_MODULE( FT_Driver_ClassRec, pcf_driver_class )
#endif
#if defined(FT2_DRIVER_CID) || defined(FT2_DRIVER_TYPE1)
FT_USE_MODULE( FT_Module_Class, psaux_module_class )
#endif
#if defined(FT2_DRIVER_OPENTYPE) || defined(FT2_DRIVER_TRUETYPE) || defined(FT2_DRIVER_TYPE1)
FT_USE_MODULE( FT_Module_Class, psnames_module_class )
#endif
#if defined(FT2_DRIVER_OPENTYPE) || defined(FT2_DRIVER_TYPE1)
FT_USE_MODULE( FT_Module_Class, pshinter_module_class )
#endif
FT_USE_MODULE( FT_Renderer_Class, ft_raster1_renderer_class )
#ifdef FT2_DRIVER_SFNT
FT_USE_MODULE( FT_Module_Class, sfnt_module_class )
#endif
#ifdef FT2_SUPPORT_SMOOTH
FT_USE_MODULE( FT_Renderer_Class, ft_smooth_renderer_class )
FT_USE_MODULE( FT_Renderer_Class, ft_smooth_lcd_renderer_class )
FT_USE_MODULE( FT_Renderer_Class, ft_smooth_lcdv_renderer_class )
#endif
#ifdef FT2_DRIVER_BDF
FT_USE_MODULE( FT_Driver_ClassRec, bdf_driver_class )
#endif

/* EOF */
